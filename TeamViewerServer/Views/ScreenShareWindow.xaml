<Window x:Class="TeamViewerServer.Views.ScreenShareWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TeamViewerServer.Views"
        mc:Ignorable="d"
        Title="ScreenShareWindow" Height="450" Width="800">
    <StackPanel>
        <Image
            Height="430"
            Width="780"
            Source="{Binding ImagePath,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}">
            
        </Image>
    </StackPanel>
</Window>
